  <script type='text/javascript'>
	$('.main .main_r .industry_img ul li').hover(function(){
		var _index=$(this).index();
		for(var i=0;i<5;i++){
			if(i<=_index){
				$('.main .main_r .industry_img ul li').eq(i).stop().animate({left:i*100+'px'},500);
			}
			else{$('.main .main_r .industry_img ul li').eq(i).stop().animate({left:i*100+186+'px'},500);
		};
		}
	})
  var jpg=new Array();
	jpg[0]="url(images/banner.jpg)";
	jpg[1]="url(images/banner1.jpg)";
	jpg[2]="url(images/banner2.jpg)";
	var timg = document.getElementById("bg");
	var x=0;
	function huan(){
		x++;
		if(x==jpg.length){
			x=0;
		}
		timg.style.backgroundImage=jpg[x];
		window.setTimeout('huan()',3000);
	}
	function btn(m)
{   
    x = x+m;
    if(x < 0)
    {
        x = jpg.length-1;
    }
    else if(x >= jpg.length)
    {
        x = 0;
    }
    timg.style.backgroundImage=jpg[x];
}
window.setTimeout('huan()',3000);
  </script>
