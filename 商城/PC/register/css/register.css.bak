#box{
	width:100%;
	height:600px;}
#box .top{
	height:80px;}
#box .top .logo{width:70px;
	height:70px;
	background:url('../images/logo.png');
	margin-left:22%;}
#box .top .icon .icon_{
	width:138px;
	height:70px;
	border:1px solid #fff;
	margin-left:50%;
