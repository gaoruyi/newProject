	top:12px;}
#box .top .icon .icon_ img,#box .top .icon .icon_1 img,#box .top .icon .icon_2 img{
	margin-top:20px;
	float:left;}
#box .top .icon .icon_ h1,#box .top .icon .icon_1 h1,#box .top .icon .icon_2 h1{
	color:#2a7d1b;
	line-height:74px;
	float:left;}
#box .top .icon .icon_ h4,#box .top .icon .icon_1 h4,#box .top .icon .icon_2 h4{
