	jpg[2]="url(images/banner2.jpg)";
	var timg = document.getElementById("bg");
	var x=0;
	function huan(){
		x++;
		if(x==jpg.length){
			x=0;
		}
		timg.style.backgroundImage=jpg[x];
		window.setTimeout('huan()',3000);
	}
	function btn(m)
{   
    x = x+m;
    if(x < 0)
    {
        x = jpg.length-1;
    }
    else if(x >= jpg.length)
    {
        x = 0;
    }
    timg.style.backgroundImage=jpg[x];
}
window.setTimeout('huan()',3000);
